{{#if description}}
/**
 * {{{description}}}
 */
{{/if}}
export type {{{name}}} = {
    {{#each properties}}
    {{#if description}}
    /**
     * {{{description}}}
     */
    {{/if}}
    {{>isReadOnly}}{{{name}}}{{>isRequired}}: {{>type parent=../name}};
    {{/each}}
}

export namespace {{{name}}} {

    /**
     * the discriminator for the model if required for more complex api's
     */
    export const discriminator = '{{{name}}}';

    {{#each enums}}
    {{#if description}}
    /**
     * {{{description}}}
     */
    {{/if}}
    export enum {{{name}}} {
        {{#each enum}}
        {{{name}}} = {{{value}}},
        {{/each}}
    }

    {{/each}}

}
