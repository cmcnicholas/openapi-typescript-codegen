{{#if description}}
/**
 * {{{description}}}
 */
{{/if}}
export type {{{name}}} = {{>type parent=name}};

export namespace {{{name}}} {

    /**
     * the discriminator for the model if required for more complex api's
     */
    export const discriminator = '{{{name}}}';

    {{#unless @root.useUnionTypes}}
    {{#each enums}}
    {{#if description}}
    /**
     * {{{description}}}
     */
    {{/if}}
    export enum {{{name}}} {
        {{#each enum}}
        {{{name}}} = {{{value}}},
        {{/each}}
    }

    {{/each}}
    {{/unless}}

}
